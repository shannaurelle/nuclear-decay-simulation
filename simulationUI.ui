<?xml version='1.0' encoding='utf-8'?>
<interface>
  <object class="ttk.Frame" id="MainFrame">
    <property name="cursor">arrow</property>
    <property name="height">500</property>
    <property name="padding">15</property>
    <property name="width">300</property>
    <layout>
      <property name="column">0</property>
      <property name="propagate">True</property>
      <property name="row">0</property>
    </layout>
    <child>
      <object class="ttk.Label" id="Label_1">
        <property name="background">#ffffff</property>
        <property name="compound">top</property>
        <property name="cursor">based_arrow_down</property>
        <property name="font">{Yu} 20 {}</property>
        <property name="justify">center</property>
        <property name="padding">15</property>
        <property name="relief">groove</property>
        <property name="takefocus">false</property>
        <property name="text" translatable="yes">Alpha Decay Simulator</property>
        <property name="width">18</property>
        <layout>
          <property name="column">0</property>
          <property name="columnspan">5</property>
          <property name="ipadx">5</property>
          <property name="ipady">5</property>
          <property name="padx">5</property>
          <property name="pady">5</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="Label_2">
        <property name="compound">right</property>
        <property name="font">{Yu} 16 {}</property>
        <property name="padding">15</property>
        <property name="text" translatable="yes">Instructions</property>
        <layout>
          <property name="column">0</property>
          <property name="columnspan">5</property>
          <property name="propagate">True</property>
          <property name="row">1</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="Label_3">
        <property name="font">{@Yu Gothic UI Semibold} 12 {bold italic}</property>
        <property name="text" translatable="yes">Supply necessary information inside the text boxes.</property>
        <layout>
          <property name="column">0</property>
          <property name="columnspan">5</property>
          <property name="ipadx">5</property>
          <property name="padx">5</property>
          <property name="propagate">True</property>
          <property name="row">2</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="Label_4">
        <property name="font">{@Yu Gothic UI Semibold} 12 {bold italic}</property>
        <property name="relief">flat</property>
        <property name="text" translatable="yes">Press button to start simulation.</property>
        <layout>
          <property name="column">0</property>
          <property name="columnspan">5</property>
          <property name="ipadx">5</property>
          <property name="ipady">5</property>
          <property name="padx">5</property>
          <property name="pady">5</property>
          <property name="propagate">True</property>
          <property name="row">3</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="Label_5">
        <property name="anchor">w</property>
        <property name="font">{@Yu Gothic UI Semibold} 12 {bold}</property>
        <property name="relief">flat</property>
        <property name="text" translatable="yes">Number of particles: </property>
        <layout>
          <property name="column">0</property>
          <property name="columnspan">2</property>
          <property name="ipadx">5</property>
          <property name="ipady">5</property>
          <property name="padx">5</property>
          <property name="pady">5</property>
          <property name="propagate">True</property>
          <property name="row">4</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Entry" id="noOfParticles">
        <property name="exportselection">false</property>
        <property name="font">{@Yu Gothic UI Semibold} 12 {}</property>
        <property name="justify">center</property>
        <property name="takefocus">true</property>
        <property name="validate">focusin</property>
        <property name="width">18</property>
        <layout>
          <property name="column">2</property>
          <property name="columnspan">3</property>
          <property name="ipadx">5</property>
          <property name="ipady">5</property>
          <property name="padx">5</property>
          <property name="pady">5</property>
          <property name="propagate">True</property>
          <property name="row">4</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="Label_6">
        <property name="anchor">w</property>
        <property name="compound">top</property>
        <property name="cursor">based_arrow_down</property>
        <property name="font">{@Yu Gothic UI Semibold} 12 {}</property>
        <property name="text" translatable="yes">Temperature:</property>
        <layout>
          <property name="column">0</property>
          <property name="columnspan">2</property>
          <property name="ipadx">5</property>
          <property name="ipady">5</property>
          <property name="padx">5</property>
          <property name="pady">5</property>
          <property name="propagate">True</property>
          <property name="row">5</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Entry" id="temperature">
        <property name="cursor">based_arrow_up</property>
        <property name="font">{@Yu Gothic UI Semibold} 12 {}</property>
        <property name="justify">center</property>
        <property name="validate">none</property>
        <property name="width">18</property>
        <layout>
          <property name="column">2</property>
          <property name="columnspan">3</property>
          <property name="ipadx">5</property>
          <property name="ipady">5</property>
          <property name="padx">5</property>
          <property name="pady">5</property>
          <property name="propagate">True</property>
          <property name="row">5</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="goButton">
        <property name="command">simulate</property>
        <property name="compound">bottom</property>
        <property name="state">normal</property>
        <property name="text" translatable="yes">Go</property>
        <layout>
          <property name="column">2</property>
          <property name="columnspan">5</property>
          <property name="ipadx">5</property>
          <property name="ipady">5</property>
          <property name="padx">5</property>
          <property name="pady">5</property>
          <property name="propagate">True</property>
          <property name="row">6</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="Label_7">
        <property name="anchor">w</property>
        <property name="borderwidth">5</property>
        <property name="font">{@Microsoft YaHei Light} 8 {}</property>
        <property name="justify">left</property>
        <property name="takefocus">true</property>
        <property name="text" translatable="yes">simulation made by Ripalda, Castro, and Macababbad</property>
        <layout>
          <property name="column">1</property>
          <property name="columnspan">4</property>
          <property name="ipadx">5</property>
          <property name="ipady">5</property>
          <property name="propagate">True</property>
          <property name="row">7</property>
        </layout>
      </object>
    </child>
  </object>
</interface>
